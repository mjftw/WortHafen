{
  "name": "worthafen",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:migrate": "ts-node drizzle/migrate.js",
    "db:push": "dotenv drizzle-kit push:pg",
    "db:generate": "dotenv drizzle-kit generate:pg",
    "db:studio": "yarn db:up && dotenv drizzle-kit studio",
    "db:cli": "yarn db:up && dotenv pgcli $DATABASE_URL",
    "db:up": "docker compose up --wait -d postgres",
    "db:reset": "docker compose down --volumes && yarn db:up",
    "dev": "yarn db:up && yarn db:push && next dev",
    "env:pull": "vercel env pull .env",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^0.3.6",
    "@planetscale/database": "^1.11.0",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.43.6",
    "@trpc/next": "^10.43.6",
    "@trpc/react-query": "^10.43.6",
    "@trpc/server": "^10.43.6",
    "drizzle-orm": "^0.29.3",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "nextjs-cors": "^2.2.0",
    "pg": "^8.11.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^2.2.1",
    "swagger-ui-react": "^5.10.5",
    "trpc-openapi": "^1.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.7",
    "@types/node": "^18.17.0",
    "@types/pg": "^8.10.9",
